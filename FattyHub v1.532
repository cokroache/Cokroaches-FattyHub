					Change=1
				end
			end
			if(NeutralAnims)then	
				if(State == 'Idle')then
					local Alpha = .2
					Animate(NK,CF.N(0,1.48,-0.1)*CF.A(M.R(-10 + 10 * M.S(Sine/15)),M.R(0),M.R(0)),Alpha,'Lerp')
