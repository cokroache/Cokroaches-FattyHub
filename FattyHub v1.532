							if Input.KeyCode == Enum.KeyCode.C then
								VirtualBody:MoveTo(Mouse.Hit.p)
								VirtualRig:MoveTo(Mouse.Hit.p)
							end
						end
						if Input.KeyCode == Enum.KeyCode.LeftShift or Input.KeyCode == Enum.KeyCode.ButtonR2 then
